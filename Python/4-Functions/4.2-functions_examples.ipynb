   "source": [
    "## Temperature Conversion"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def temp_conv(temp,unit):\n",
    "    if unit==\"C\":\n",
    "        return temp * 9/5 +32 # C to F\n",
    "    elif unit==\"F\":\n",
    "        return (temp-32)*5/9 # F to C\n",
    "    else:\n",
    "        return None"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "77.0째F.\n",
      "25.0째C.\n"
     ]
    }
   ],
   "source": [
    "print(f\"{temp_conv(25, 'C')}째F.\")\n",
    "print(f\"{temp_conv(77, 'F')}째C.\")"
   ]
  }
